Binary Search Practice Strivers SDE Sheet

Find unique number 
class Solution {
public:
    int singleNonDuplicate(vector<int>& nums) {
        int low=0,high=nums.size()-1;
        int ans=-1;
        if(nums.size()==1){
            return nums[0];
        }
        while(low<=high){
            int mid=low+(high-low)/2;
            if(mid%2==0){
                if(mid+1<=nums.size()-1 && nums[mid]==nums[mid+1]){
                    low=mid+1;
                }
                else if(mid-1>=0 && nums[mid]==nums[mid-1]){
                    high=mid-1;
                }
                else {
                    ans=nums[mid];
                    break;
                }
            }
            else if(mid%2!=0){
                if(mid-1>=0 && nums[mid]==nums[mid-1]){
                    low=mid+1;
                }
                else if(mid+1<=nums.size()-1 && nums[mid]==nums[mid+1]){
                    high=mid-1;
                }
                else {
                    ans=nums[mid];
                    break;
                }
            }
            
        }
        return ans;
    }
};
